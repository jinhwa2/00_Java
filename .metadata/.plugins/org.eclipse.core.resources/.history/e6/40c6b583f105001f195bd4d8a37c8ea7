package com.kh.StackEx;

import java.util.LinkedList;
import java.util.Queue;

public class QueuePre {
//main
	public static void main(String[] args) {
	Queue<Integer> 큐우 = new LinkedList<>();	
	
	//offer 80 60 30 20
	큐우.offer(80);
	큐우.offer(60);
	큐우.offer(30);
	큐우.offer(20);
		
	//poll 데이터 확인 및 제거
	큐우.poll();
	System.out.println("큐에 제거된 데이터는? "+큐우.poll());
	
	//peek 맨 앞 데이터 확인
	큐우.peek();
	System.out.println("큐우 맨 앞 데이터 확인: "+큐우.peek());
		
	//isEmpty 비어있는지 확인
	boolean 비어있낭 = 큐우.isEmpty();
	System.out.println("비어있나: "+비어있낭);


	
	
	
	
	
	
	
	}

}
